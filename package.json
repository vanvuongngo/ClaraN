{
  "name": "clara-n",
  "description": "Your fully local AI Superstack - Privacy-First, On-Premises, Limitless",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.12.2"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "android:dev": "pnpm --filter clara-n tauri android dev",
    "android:build": "pnpm --filter clara-n tauri android build",
    "build": "pnpm --filter clara-n tauri build",
    "build.macos": "pnpm --filter clara-n tauri build --bundles app",
    "build.dmg": "pnpm --filter clara-n tauri build --bundles dmg",
    "dev": "pnpm --filter clara-n tauri dev",
    "ios:dev": "pnpm --filter clara-n tauri ios dev",
    "web:dev": "pnpm --filter clara-n dev"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.15.0",
    "@builder.io/qwik-city": "^1.15.0",
    "@eslint/js": "^9.32.0",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/cli": "^2.7.1",
    "@types/node": "^22.16.0",
    "daisyui": "^5.0.47",
    "eslint": "^9.32.0",
    "eslint-plugin-qwik": "^1.15.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "qwik-localstorage": "^1.2.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "undici": "^7.12.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
