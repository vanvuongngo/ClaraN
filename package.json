{
  "name": "clara-n",
  "description": "Your fully local AI Superstack - Privacy-First, On-Premises, Limitless",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.12.2"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm --filter clara-n tauri build",
    "build:android": "pnpm --filter clara-n tauri android build",
    "build:macos": "pnpm --filter clara-n tauri build --bundles app",
    "build:dmg": "pnpm --filter clara-n tauri build --bundles dmg",
    "dev": "pnpm --filter clara-n tauri dev",
    "dev:android": "pnpm --filter clara-n tauri android dev",
    "dev:ios": "pnpm --filter clara-n tauri ios dev",
    "dev:web": "pnpm --filter clara-n dev",
    "i18n-extract": "pnpm --filter clara-n i18n-extract",
    "lint": "pnpm --filter clara-n lint",
    "retag": "git tag -d ${VERSION} && git push origin :${VERSION} && git tag ${VERSION} && git push origin ${VERSION}"
  },
  "dependencies": {
    "@angular/localize": "^20.2.3"
  },
  "devDependencies": {
    "@angular/compiler": "^20.2.3",
    "@angular/compiler-cli": "^20.2.3",
    "@builder.io/qwik": "^1.16.0",
    "@builder.io/qwik-city": "^1.16.0",
    "@builder.io/qwik-react": "^0.5.6",
    "@eslint/js": "^9.34.0",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/cli": "^2.8.3",
    "@tauri-apps/plugin-clipboard-manager": "~2.3.0",
    "@tauri-apps/plugin-log": "^2.6.0",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@tauri-apps/plugin-os": "^2.3.1",
    "@tauri-apps/plugin-store": "^2.4.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "@types/node": "^22.18.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "daisyui": "^5.0.54",
    "eslint": "^9.34.0",
    "eslint-plugin-qwik": "^1.16.0",
    "globals": "^16.3.0",
    "lucide-react": "^0.542.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "qwik-localstorage": "^1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "undici": "^7.15.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}